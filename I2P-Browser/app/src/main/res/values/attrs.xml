<resources>
    <declare-styleable name="ResizableKeyboardViewDelegate">
        <attr name="viewToHideWhenActivated" format="reference" />
        <attr name="animate" format="boolean" />
    </declare-styleable>

    <declare-styleable name="AnimatedProgressBar">
        <attr name="wrapShiftDrawable" format="boolean" />
        <attr name="shiftDuration" format="reference" />
        <attr name="shiftInterpolator" format="reference" />
    </declare-styleable>
    <declare-styleable name="ConnectionLimitPreference">
        <attr name="clp_valueInTitle" format="boolean" />
    </declare-styleable>

    <declare-styleable name="DrawShadowFrameLayout">
        <attr name="shadowDrawable" format="reference" />
        <attr name="shadowVisible" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SeekBarPreference">
        <attr name="msbp_maxValue" format="reference|integer"/>
        <attr name="msbp_defaultValue" format="reference|integer"/>
        <attr name="msbp_measurementUnit" format="reference|string"/>
    </declare-styleable>

</resources>